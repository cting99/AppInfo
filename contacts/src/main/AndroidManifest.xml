<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cting.com.robin.contacts">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >


        <activity
            android:name=".activities.ContactListActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/activity_contact_list">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cting.com.robin.support.commom.activities.TextActivity"
            android:label="@string/activity_text" />

    </application>
</manifest>
